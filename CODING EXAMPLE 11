package selfProgramming;

public class Example11 {
  public static void main(String[]args) {
	  
	  for(int num =2; num<20; num++) {
		  
		  num = num*2;
		  
		  if(num<15)
			  System.out.println("Small number = " +num);
		  
		  else 
			  System.out.print("Big number = " +num);
	  }
	  
	  
	  
	  
	  
	  
	  
	  
	  
	  
}}
